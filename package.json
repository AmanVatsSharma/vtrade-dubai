{
  "name": "trading-app-platform",
  "version": "0.1.0",
  "description": "trading app platform with nextjs, prisma, typescript, tailwindcss",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "clean:generated": "rm -rf generated prisma/generated",
    "type-check": "tsc --noEmit",
    "postinstall": "prisma generate",
    "test": "jest --config jest.config.cjs --forceExit",
    "test:auth": "node scripts/run-auth-tests.js",
    "test:watch": "jest --config jest.config.cjs --watch",
    "test:coverage": "jest --config jest.config.cjs --coverage --forceExit"
  },
  "dependencies": {
    "@apollo/client": "^4.0.2",
    "@auth/prisma-adapter": "^1.0.6",
    "@aws-sdk/client-s3": "^3.901.0",
    "@aws-sdk/client-sns": "^3.888.0",
    "@aws-sdk/s3-request-presigner": "^3.901.0",
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "6.18.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-email/render": "^1.2.3",
    "@rupeezy/jsvortex": "^2.0.2",
    "@sentry/node": "^8.35.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.57.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@vercel/functions": "^3.4.1",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "canvas-confetti": "^1.9.3",
    "class-variance-authority": "^0.7.1",
    "critters": "^0.0.25",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.14",
    "graphql": "^16.11.0",
    "graphql-scalars": "^1.24.2",
    "jsonwebtoken": "^9.0.2",
    "lightweight-charts": "^5.0.8",
    "lucide-react": "^0.536.0",
    "next": "14.2.5",
    "next-auth": "^5.0.0-beta.21",
    "next-themes": "^0.4.6",
    "pino": "^9.4.0",
    "prom-client": "^15.1.1",
    "protobufjs": "^7.5.4",
    "react": "^18",
    "react-day-picker": "^9.11.0",
    "react-dom": "^18",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "react-is": "^19.2.4",
    "react-responsive": "^10.0.1",
    "react-swipeable": "^7.0.2",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^2.1.0",
    "recharts": "^3.2.1",
    "resend": "^6.0.3",
    "rxjs": "^7.8.2",
    "socket.io-client": "^4.8.1",
    "stocksocket": "^2.1.2",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.3",
    "uuid": "^11.1.0",
    "vaul": "^1.1.2",
    "ws": "^8.18.3",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.9",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/ws": "^8.18.1",
    "dotenv": "^17.2.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "jest": "^30.2.0",
    "postcss": "^8.5",
    "prisma": "6.18.0",
    "tailwindcss": "^4.1.9",
    "tw-animate-css": "1.3.3",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "nextjs",
    "prisma",
    "typescript",
    "tailwindcss"
  ],
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.ts"
    ],
    "testMatch": [
      "**/tests/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "actions/**/*.ts",
      "lib/**/*.ts",
      "components/**/*.tsx",
      "!**/*.d.ts",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    }
  }
}
